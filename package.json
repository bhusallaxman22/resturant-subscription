{
  "name": "resturant-subscription",
  "version": "1.0.0",
  "description": "Full-stack project with React frontend and Express backend",
  "scripts": {
    "start:frontend": "cd frontend && npm run dev",
    "build:frontend": "cd frontend && npm run build",
    "start:backend": "cd server && npm install && npm run start",
    "dev:backend": "cd server && npm run dev",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:frontend": "cd frontend && npm run lint",
    "lint:backend": "cd server && npm run lint",
    "format": "prettier . --write",
    "format:frontend": "cd frontend && npm run format",
    "format:backend": "cd server && npm run format",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "cd frontend && npm run test",
    "test:backend": "cd server && npm run test",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.32.2",
    "prettier": "^2.8.8"
  },
  "dependencies": {
    "dotenv": "^16.4.7"
  }
}
